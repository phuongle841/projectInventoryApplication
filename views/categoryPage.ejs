<html>
  <header>
    <title>Category in shop</title><link rel="stylesheet" href="/index.css" />
  </header>
  <body>
    <h1><%= name %></h1>
    <%- include("partial/nav.ejs") %>
    <div class="container">
      <% products.forEach(function(product){ %>
      <li>
        <a href="/products/<%= product.id %>"><%= product.name %></a>
      </li>
      <% }); %>
    </div>
    <a href="/categories/<%= id %>/update">update</a>
    <a href="/categories/<%= id %>/delete">delete</a>
  </body>
</html>
