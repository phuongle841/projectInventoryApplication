<html>
  <head>
    <link rel="stylesheet" href="/index.css" />
  </head>
  <body>
    <h1><%= name %></h1>
    <%- include("partial/nav.ejs") %>
    <div class="container">
      <% products.forEach((product)=>{ %>
      <li>
        <a href="/products/<%= product.id %>"><%= product.product_name %></a>
      </li>
      <% }); %>
    </div>
    <a href="/sellers/<%= id %>/update">update</a>
    <a href="/sellers/<%= id %>/delete">delete</a>
  </body>
</html>
